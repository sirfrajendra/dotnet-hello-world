{
  "family": "dotnet-task",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": "1024",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "dotnet-app",
      "image": "652873843596.dkr.ecr.us-east-1.amazonaws.com/dotnet-hello-world:latest",
      "essential": true,
      "memory": 1024,
      "cpu": 1024,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
